{
  "name": "jack-henry-design-system",
  "description": "Jack Henry's design system web components.",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Banno/jack-henry-design-system.git"
  },
  "contributors": [
    "Audrey Bissiere-Grote",
    "Brad McCormick",
    "Maya Buser De"
  ],
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "install:all": "pnpm install",
    "install:jh-elements": "pnpm install --filter @jack-henry/jh-elements",
    "install:jh-tokens": "pnpm install --filter @jack-henry/jh-tokens",
    "install:jh-icons": "pnpm install --filter @jack-henry/jh-icons",
    "generate:jh-elements": "pnpm run --filter @jack-henry/jh-elements generate",
    "storybook:jh-elements": "pnpm run --filter @jack-henry/jh-elements storybook",
    "analyze:jh-elements": "pnpm run --filter @jack-henry/jh-elements analyze",
    "build:jh-tokens": "pnpm run --filter @jack-henry/jh-tokens build",
    "build:jh-icons": "pnpm run --filter @jack-henry/jh-icons build-wc",
    "changeset:version": "changeset version && pnpm install --lockfile-only",
    "license:check": "node bin/license-tool.js check",
    "license:fix": "node bin/license-tool.js fix"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.2",
    "glob": "11.0.0"
  }
}